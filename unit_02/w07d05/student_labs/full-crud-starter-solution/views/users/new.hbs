<h1>New User</h1>
<form action="/users" method="POST">
  <div>
    <label for="first_name">
      First Name
    </label>
    <input type="text" name="first_name"/>
  </div>
  <div>
    <label for="email">
      Email
    </label>
    <input type="text" name="email"/>
  </div>
  <div>
    <input type="submit" value="Create User"/>
  </div>
</form>
